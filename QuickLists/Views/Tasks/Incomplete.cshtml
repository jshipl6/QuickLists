@model List<QuickLists.Models.TaskItem>

<h2>Incomplete Tasks</h2>

<table class="table">
    <thead>
        <tr>
            <th>Title</th>
            <th>Complete</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Title</td>
                <td>@item.IsComplete</td>
            </tr>
        }
    </tbody>
</table>